						["Connection", "{connection}"]
						["X-Powered-By", "PHP/5.5.9-1ubuntu4.5"]
					]
				}
			]
			*/
		}
		upnp = {
			root = "var/dionaea/wwwroot"
			max-request-size = "32768" // maximum size in kbytes of the request (32MB)
			personalities-enable = "default"
			personalities = {
